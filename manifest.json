{
  "manifest_version": 3,
  "name": "New Tab",
  "version": "1.0",
  "description": "Opens a video when you open a new tab",
  "permissions": ["activeTab", "webNavigation"],
  "background": {
    "service_worker": "newtab.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "48": "icon.png"
  }
}